{
  "name": "sun-nation-website",
  "version": "1.0.0",
  "description": "Sun Nation - Static site built with Jekyll",
  "type": "module",
  "scripts": {
    "dev": "bundle exec jekyll serve",
    "build": "bundle exec jekyll build",
    "import:wordpress": "node scripts/import-wordpress.cjs",
    "test": "npm run test:unit && npm run test:integration",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mfrench71/sun-nation-website.git"
  },
  "author": "Matthew French",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0",
    "js-yaml": "^4.1.0",
    "turndown": "^7.2.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "happy-dom": "^20.0.10",
    "husky": "^9.1.7",
    "netlify-cli": "^23.9.2",
    "vitest": "^1.0.4"
  }
}
